{
  "name": "planned-meals",
  "version": "1.0.0",
  "description": "A meal-planning app",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config app.js",
    "test": "jest",
    "dev": "nodemon -r dotenv/config server.js",
    "dbcreaterecipetable": "node -r dotenv/config ./db/scripts/createRecipeDb.js",
    "dbdeleterecipetable": "node -r dotenv/config ./db/scripts/deleteRecipeDb.js",
    "dbresetrecipetable": "npm run dbdeleterecipetable && npm run dbcreaterecipetable.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onlyasmalllizard/planned-meals.git"
  },
  "author": "Lizard Morrow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onlyasmalllizard/planned-meals/issues"
  },
  "homepage": "https://github.com/onlyasmalllizard/planned-meals#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "jest": "^27.1.0",
    "supertest": "^6.1.6"
  }
}
